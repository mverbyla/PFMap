Package: pathogenflows
Title: Pathogen Flows for Onsite Sanitation Systems
Version: 0.0.0.9004
Authors@R: 
    person(given = "Matthew",
           family = "Verbyla",
           role = c("aut","cre"),
           email = "mverbyla@sdsu.edu",
